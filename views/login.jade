              a.external(href='/#home-slider') Home
            li
              a.external(href='index.html#work') Our Work
            li
              a.external(href='index.html#about') About Us
            li
              a.external(href='index.html#contact') Contact
            li.current
              a(href='/shortcodes') Shortcodes
    // End Header
    // Our Shortcodes Section
    #contact.page
          .container
            // Title Page
            .row
              .span12
                .title-page
                  h2.title Login Page
                  h3.title-description 로그인페이지입니다.
            // End Title Page
            // Contact Form
            .row
             .span5
              .title-page
                form(method='post', action='/login')
                 .container
                  label
                    b Username
                  p
                    input#contact_id(type='text', placeholder='Id', name='id',required)
                  label
                    b Password
                  p
                    input#contact_password(type='password', placeholder='Password', name='password',required)
                  button.button(type='submit') Login
            // End Contact Form
